<script>
// Countdown Timer
const expeditionDate = new Date('2026-07-12T00:00:00');

function updateCountdown() {
  const now = new Date();
  const diff = expeditionDate - now;

  if (diff <= 0) {
    document.getElementById('days').textContent = '0';
    document.getElementById('hours').textContent = '0';
    document.getElementById('minutes').textContent = '0';
    document.getElementById('seconds').textContent = '0';
    return;
  }

  const days = Math.floor(diff / (1000 * 60 * 60 * 24));
  const hours = Math.floor((diff % (1000 * 60 * 60 * 24)) / (1000 * 60 * 60));
  const minutes = Math.floor((diff % (1000 * 60 * 60)) / (1000 * 60));
  const seconds = Math.floor((diff % (1000 * 60)) / 1000);

  document.getElementById('days').textContent = days;
  document.getElementById('hours').textContent = hours;
  document.getElementById('minutes').textContent = minutes;
  document.getElementById('seconds').textContent = seconds;
}

// Cost Estimator
const costs = {
  '2027': { min: 1600, max: 2100, months: 8 },
  '2028': { min: 3000, max: 3800, months: 10 },
  '2029': { min: 1200, max: 2000, months: 10 }
};

function updateCostEstimator() {
  const year = document.getElementById('cost-year').value;
  const fundraising = parseInt(document.getElementById('cost-fundraising').value) || 0;
  const cost = costs[year];

  const avgCost = (cost.min + cost.max) / 2;
  const afterFundraising = Math.max(0, avgCost - fundraising);
  const monthly = afterFundraising / cost.months;

  document.getElementById('estimated-total').textContent = '$' + afterFundraising.toLocaleString();
  document.getElementById('payment-amount').textContent = '$' + Math.round(monthly);
}

document.addEventListener('DOMContentLoaded', function() {
  updateCountdown();
  setInterval(updateCountdown, 1000);
  updateCostEstimator();

  document.getElementById('cost-year').addEventListener('change', updateCostEstimator);
  document.getElementById('cost-fundraising').addEventListener('input', updateCostEstimator);
});
</script>

